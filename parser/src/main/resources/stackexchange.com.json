{
  "name": "stackexchange.com",
  "url": "http://stackexchange.com",
  "path_provider": "css",
  "page": [
    {
      "type": "front_page",
      "url": "http://stackexchange.com/",
      "attrs": [
        {
          "path": ".question-link",
          "attr": "href",
          "type": "question_page"
        }
      ]
    },
    {
      "type": "question_page",
      "attrs": [
        {
          "path": "h1 > .question-hyperlink",
          "attr": "text",
          "type": "question_title"
        },
        {
          "path": ".question .post-text",
          "attr": "text",
          "type": "question_description"
        },
        {
          "path": "#question-header .question-hyperlink",
          "attr": "href",
          "type": "question_page"
        }
      ]
    }
  ]
}