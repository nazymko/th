{
  "url": "http://tcb.vn.ua",
  "name": "TK Bidnyajka",
  "selector": "css",
  "meta": {
    "topics": [
      "programming"
    ],
    "type": [
      "q&a",
      "excursion",
      "bus_travel"
    ],
    "languages": [
      "eng"
    ]
  },
  "page": [
    {
      "type": "article",
      "unique_visit": true,
      "parent": "article",
      "attrs": [
        {
          "path": ".inforr",
          "attr": "text",
          "type": "parser#info-block",
          "persist": false,
          "format": "YYYY-MM-DD hh:mm:ss",
          "regexp": [
            {
              "type": "trip-price",
              "expression": ".*Орг\\.збір:\\s(\\d*)\\s(\\S*)\\..*",
              "persist": true,
              "format": "YYYY-MM-DD hh:mm:ss",
              "group": [
                {
                  "order": 1,
                  "nature": "number",
                  "type": "price",
                  "format": "nnnnn.nn",
                  "persist": true
                }
              ]
            },
            {
              "type": "date",
              "format": "YYYY-MM-DD hh:mm:ss",
              "expression": "Дата:\\s*(((\\d\\d)(([02468][048])|([13579][26]))-02-29)|(((\\d\\d)(\\d\\d)))-((((0\\d)|(1[0-2]))-((0\\d)|(1\\d)|(2[0-8])))|((((0[13578])|(1[02]))-31)|(((0[1,3-9])|(1[0-2]))-(29|30)))))\\s([01]\\d|2[0-3]):([0-5]\\d):([0-5]\\d)"
            },
            {
              "type": "phone",
              "expression": "([-.\\s]?\\(?\\d{1,3}?\\)?[-.\\s]?\\d{1,4}[-.\\s]?\\d{1,4}[-.\\s]?\\d{1,9})"
            }
          ]
        }
      ]
    },
    {
      "type": "slave_page",
      "parent": "article",
      "attrs": [
        {
          "path": ".tripak a",
          "attr": "href",
          "type": "article"
        }
      ]
    }
  ]
}