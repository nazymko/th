{
  "name": "kraina-ua.com",
  "url": "http://kraina-ua.com/",
  "path_provider": "css",
  "meta": {
    "topics": [
      "travelling"
    ],
    "type": [
      "excursion",
      "bus_travel"
    ],
    "languages": [
      "ua"
    ]
  },
  "page": [
    {
      "type": "front_page",
      "url": "http://kraina-ua.com/",
      "attrs": [
        {
          "path": "body > header > div > nav > ul > li.menu__li.menu__li_aux.menu__li_ic1 > span > a",
          "attr": "href",
          "type": "calendar"
        }
      ]
    },
    {
      "type": "calendar",
      "attrs": [
        {
          "path": "body > div.container > div > div.layout__col-l > div > form > div.tours-cnt > div.listPagination > table > tbody > tr > td.next > a",
          "attr": "href",
          "type": "calendar"
        },
        {
          "path": ".tours-lst .tours-lst__col-2 .tours-lst__ac a",
          "attr": "href",
          "type": "article"
        }
      ]
    },
    {
      "type": "article",
      "unique": true,
      "attrs": [
        {
          "path": ".item-img > img",
          "attr": "src",
          "type": "big-image",
          "persist": true
        },
        {
          "path": ".item-view .item-view__l",
          "attr": "html",
          "type": "short-details",
          "persist": true
        },
        {
          "path": "#w-tab-1",
          "attr": "html",
          "type": "big-details",
          "persist": true
        },
        {
          "path": ".date-lst-lbl.date-lst-lbl-1",
          "attr": "text",
          "type": "trip-date",
          "format": "MM.dd",
          "persist": true
        },
        {
          "path": ".item-view__price",
          "attr": "text",
          "type": "trip-price",
          "persist": false,
          "regexp": [
            {
              "type": "price",
              "expression": "\\s?(\\d*)\\s(.*)",
              "group": [
                {
                  "order": 1,
                  "nature": "number",
                  "type": "price",
                  "persist": true
                },
                {
                  "order": 2,
                  "nature": "string",
                  "type": "currency",
                  "persist": true
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}