{
  "url": "http://tcb.vn.ua",
  "name": "TK Bidnyajka",
  "selector": "css",
  "meta": {
    "topics": [
      "programming"
    ],
    "type": [
      "q&a",
      "excursion",
      "bus_travel"
    ],
    "languages": [
      "eng"
    ]
  },
  "page": [
    {
      "type": "front_page",
      "url": "http://tcb.vn.ua",
      "attrs": [
        {
          "path": ".travels_meny .myul li a",
          "attr": "href",
          "type": "category_list"
        }
      ]
    },
    {
      "type": "category_list",
      "unique": false,
      "attrs": [
        {
          "path": ".tripak >a",
          "attr": "href",
          "type": "article"
        }
      ]
    },
    {
      "type": "article",
      "unique": true,
      "attrs": [
        {
          "path": ".entry td h3",
          "attr": "text",
          "type": "article-title",
          "persist": true
        },
        {
          "path": ".newcontent img",
          "attr": "src",
          "type": "article-img",
          "persist": true
        },
        {
          "path": ".newcontent a[href^=https://tcb.vn.ua]",
          "attr": "href",
          "type": "slave_page",
          "persist": false
        },
        {
          "path": ".newcontent",
          "attr": "text",
          "type": "contacts",
          "persist": false,
          "regexp": [
            {
              "type": "contact-phone",
              "expression": "(\\s*\\d{10})+",
              "group": [
                {
                  "order": 1,
                  "nature": "string",
                  "type": "phone",
                  "persist": true
                },
                {
                  "order": 2,
                  "nature": "string",
                  "type": "phone",
                  "persist": true
                }
              ]
            },
            {
              "type": "category",
              "expression": "Тип поїздки:\\s(\\S*)\\s",
              "persist": true
            },
            {
              "type": "contact-name",
              "expression": "Відповідальний:\\s*(.*)\\s(.*)\\s[Контакты|Контакти]",
              "group": [
                {
                  "order": 1,
                  "nature": "string",
                  "type": "firstName",
                  "persist": true
                },
                {
                  "order": 2,
                  "nature": "string",
                  "type": "lastName",
                  "persist": true
                }
              ]
            }
          ]
        },
        {
          "path": ".inforr",
          "attr": "text",
          "type": "parser#info-block",
          "persist": "false",
          "regexp": [
            {
              "type": "trip-price",
              "expression": ".*Орг\\.збір:\\s}*(\\d*)\\s(\\S*).*",
              "group": [
                {
                  "order": 1,
                  "nature": "number",
                  "type": "price",
                  "persist": true
                },
                {
                  "order": 2,
                  "nature": "enum",
                  "type": "currency",
                  "persist": true
                }
              ]
            },
            {
              "type": "date",
              "format": "YYYY-MM-DD hh:mm:ss",
              "expression": "Дата:\\s*((((\\d\\d)(([02468][048])|([13579][26]))-02-29)|(((\\d\\d)(\\d\\d)))-((((0\\d)|(1[0-2]))-((0\\d)|(1\\d)|(2[0-8])))|((((0[13578])|(1[02]))-31)|(((0[1,3-9])|(1[0-2]))-(29|30)))))\\s([01]\\d|2[0-3]):([0-5]\\d):([0-5]\\d))",
              "persist": true
            }
          ]
        }
      ]
    },
    {
      "type": "slave_page",
      "parent": "article",
      "attrs": [
        {
          "path": ".entry",
          "attr": "html",
          "type": "big-body",
          "persist": true
        }
      ]
    }
  ]
}