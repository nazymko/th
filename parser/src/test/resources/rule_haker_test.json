{
  "name": "][akep",
  "url": "https://xakep.ru/",
  "path_provider": "css",
  "page": [
    {
      "type": "front_page",
      "url": "https://xakep.ru/",
      "attrs": [
        {
          "path": ".parent-item",
          "attr": "href",
          "type": "category_list"
        }
      ]
    },
    {
      "type": "category_list",
      "attrs": [
        {
          "path": ".loop-link",
          "attr": "href",
          "type": "article"
        },
        {
          "path": ".loop-panel > .loop-info-wrapper  span.title-text",
          "attr": "text",
          "type": "link-text"
        },
        {
          "path": ".loop-image",
          "attr": "style",
          "type": "link-image"
        }
      ]
    },
    {
      "type": "article",
      "attrs": [
        {
          "path": ".entry-title",
          "attr": "text",
          "type": "article-title"
        },
        {
          "path": ".featured-image-inner > .wp-post-image",
          "attr": "src",
          "type": "article-image"
        },
        {
          "path": ".author > a",
          "attr": "text",
          "type": "article-author-name"
        },
        {
          "path": ".author > a",
          "attr": "href",
          "type": "article-author-page"
        },
        {
          "path": ".the-content",
          "attr": "text",
          "type": "article-content"
        },
        {
          "path": "a[href*=ru.wikipedia.org/wiki]",
          "attr": "href",
          "type": "wiki-page"
        }
      ]
    },
    {
      "type": "wiki-page",
      "attrs": [
        {
          "path": "#firstHeading",
          "attr": "text",
          "type": "wiki-page-title"
        }
      ]
    }
  ]
}